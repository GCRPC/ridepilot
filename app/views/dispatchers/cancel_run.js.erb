<% if @run %>
  var run_id = <%= @run.id %>;
  $('#run_trips_panel_' + run_id).remove();
  $('#runs_panel table tbody tr[data-run-id=' + run_id + ']').remove();
  $('#unassigned_trips_panel').replaceWith("<%= j render('dispatchers/unassigned_trips_panel') %>");
  configure_unassigned_trips_table();
<% end %>