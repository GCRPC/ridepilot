%ul
  - vehicle_capacity_configurations.each do |config|
    %li
      - capacity_data = config.vehicle_capacities.where("capacity is NOT NULL and capacity > 0").default_order.joins(:capacity_type).pluck("capacity_types.name", :capacity)
      - capacity_data.each do |d|
        %b= "#{d[0]}: "
        = d[1]