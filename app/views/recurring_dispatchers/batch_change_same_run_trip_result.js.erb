<% if @target_trips.any? %>
  <% if @target_run.present? %>
    var run_id = <%= @target_run.id %>;
    $('#run_trips_panel_' + run_id).replaceWith("<%= j render('recurring_dispatchers/run_trips_panel', run: @target_run) %>");
    $('#runs_panel table tbody tr[data-run-id=' + run_id + '] td.trips_count').html(<%= @target_run.reload.weekday_assignments.for_wday(@day_of_week).count%>);
  <% end %>
  $('#unassigned_trips_panel .panel-title').html("<%= j render('recurring_dispatchers/unassigned_trips_panel_title') %>");
<% end %>